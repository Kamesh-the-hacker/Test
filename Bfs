#include <stdio.h>

int queue[50], front = -1, rear = -1;
int visited[50];

void bfs(int n, int a[50][50], int start) {
    int i;
    front = rear = 0;
    queue[rear] = start;
    visited[start] = 1;

    while (front <= rear) {
        start = queue[front++];
        printf("%d ", start);
        for (i = 1; i <= n; i++) {
            if (a[start][i] && !visited[i]) {
                queue[++rear] = i;
                visited[i] = 1;
            }
        }
    }
}

int main() {
    int n, a[50][50], i, j, start;
    scanf("%d", &n);
    for (i = 1; i <= n; i++)
        for (j = 1; j <= n; j++)
            scanf("%d", &a[i][j]);
    scanf("%d", &start);
    for (i = 1; i <= n; i++) visited[i] = 0;
    bfs(n, a, start);
    return 0;
}
