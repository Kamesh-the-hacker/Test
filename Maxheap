#include <stdio.h>

#define MAX 100

int heap[MAX];
int size = 0;

void swap(int *a, int *b) {
    int t = *a;
    *a = *b;
    *b = t;
}

// Heapify up
void heapify_up(int i) {
    while (i > 1 && heap[i/2] < heap[i]) {
        swap(&heap[i/2], &heap[i]);
        i = i/2;
    }
}

// Insert into max heap
void insert(int val) {
    size++;
    heap[size] = val;
    heapify_up(size);
}

// Print heap
void printHeap() {
    printf("Max Heap: ");
    for (int i = 1; i <= size; i++)
        printf("%d ", heap[i]);
    printf("\n");
}

int main() {
    // insert elements
    insert(10);
    insert(30);
    insert(20);
    insert(5);
    insert(50);

    // print heap
    printHeap();
    return 0;
}
